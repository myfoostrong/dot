{
  "$schema": "https://opencode.ai/config.json",
  "plugin": [
    "opencode-openai-codex-auth@4.1.1",
    "opencode-gemini-auth@latest"
  ],
  "model": "openai/gpt-5.1-codex-max-medium",
  "permission": {
    "doom_loop": "ask"
  },
  "theme": "tokyonight",
  "agent": {
    "task-orchestrator": {
        "description": "This agent excels at managing multiple subagents in parallel.",
        "mode": "primary",
        "model": "openai/gpt-5.1-codex-max-low",
        "tools": {
            "write": false,
            "edit": false,
            "bash": false,
            "todoread": true,
            "todowrite": true
        },
        "prompt": "{file:./prompts/task-orchestrator.md}"
    },
    "lookup": {
      "model": "openai/gpt-5.1-codex-max-low",
      "mode": "subagent",
      "permission": {
        "edit": "deny",
      },
      "description": "This agent excels at researching and locating information. It’s optimized for finding where specific code elements are defined or used, reading and interpreting documentation, researching technical details, and retrieving code examples that demonstrate best practices or specific APIs. It’s also great at maintaining and leveraging context, helping the primary agent quickly surface relevant information from large codebases, docs, or external sources.",
      "prompt": "After conducting your research, summarize the key findings clearly and concisely. Include only the most relevant code examples, file names, and sources as needed.",
    },
    "developer": {
      "description": "Use this agent when a user requires development, optimization, or advice on code.",
      "mode": "subagent",
      "model": "opencode/big-pickle",
      "prompt": "Senior developer with extensive experience building applications with robust, scalable architectures with project-specific expertise. Develop, review, optimize, and advise on code aligned with project patterns."
    },
    "debugger": {
      "description": "Use this agent when analyzing the output of test runs and debugging the root cause of the failures.",
      "mode": "subagent",
      "model": "openai/gpt-5.1-codex-max-low",
      "prompt": "{file:./prompts/test_debug/test-failure-analyzer.md}",
      "tools": {
        "write": false,
        "edit": false,
        "bash": true,
        "todoread": true,
        "todowrite": true,
        "webfetch": true,
        "glob": true,
        "grep": true,
        "list": true
      }
    }
  },
  "mcp": {
//     "context7": {
//       "type": "remote",
//       "url": "https://mcp.context7.com/mcp",
//       "headers": {
//         "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}",
//       },
//       "enabled": false,
//     },
    "gh_grep": {
      "type": "remote",
      "url": "https://mcp.grep.app/",
      // "enabled": false,
    },
  },
